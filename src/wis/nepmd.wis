<CINCLUDE name="..\nls\wis\nepmd%NLS%.h"/>

<WARPIN>
<HEAD>

<PCK INDEX=1
     PACKAGEID="OS/2 Netlabs\Netlabs EPM Distribution\Base Application Support\%ID_VERSION_NEPMD%"
     TARGET="$(WARPIN_DEFAULTAPPSPATH)\NEPMD"
     BASE
     REQUIRES="OS/2 Netlabs\WarpIN\Base\%ID_VERSION_REQ_WARPIN%"
     SELECT
     NODESELECT
     TITLE="%PCK_1_TITLE%"
     EXECUTE="$(1)\netlabs\install\nlsetup.exe NEPMD"
     DEEXECUTE="$(1)\netlabs\install\nldeinst.exe"

     WRITEPROFILE="USER\NEPMD\Language|%NLS%"
     WRITEPROFILE="USER\NEPMD\Path|$(1)"

     CREATEOBJECT="WPFolder|%TITLE_NEPMD_FOLDER%|<WP_APPSFOLDER>|ICONVIEWPOS=22 41 65 34;CCVIEW=NO;OPEN=DEFAULT;OPEN=DEFAULT;OBJECTID=<NEPMD_FOLDER>;"
     CREATEOBJECT="WPProgram|%TITLE_NEPMD_EXECUTE%|<NEPMD_FOLDER>|PROGTYPE=PM;EXENAME=EPM.EXE;OBJECTID=<NEPMD_EXECUTE>;"
     CREATEOBJECT="WPProgram|%TITLE_NEPMD_VIEW_EPMTECH%|<NEPMD_FOLDER>|;PROGTYPE=PM;EXENAME=VIEW.EXE;PARAMETERS=$(1)\EPMBBS\BOOK\EPMTECH.INF;OBJECTID=<NEPMD_VIEW_EPMTECH>;"
     CREATEOBJECT="WPProgram|%TITLE_NEPMD_VIEW_EPMUSERS%|<NEPMD_FOLDER>|PROGTYPE=PM;EXENAME=VIEW.EXE;PARAMETERS=$(1)\EPMBBS\BOOK\EPMUSERS.INF;OBJECTID=<NEPMD_VIEW_EPMUSERS>;"
     CREATEOBJECT="WPProgram|%TITLE_NEPMD_VIEW_NEPMD%|<NEPMD_FOLDER>|PROGTYPE=PM;EXENAME=VIEW.EXE;PARAMETERS=$(1)\NETLABS\BOOK\NEPMD.INF Netlabs;ICONFILE=$(1)\NETLABS\INSTALL\ICO\HELP.ICO;OBJECTID=<NEPMD_VIEW_NEPMD>;"
     ><TINCLUDE name="..\nls\wis\desc1%NLS%.xml"/></PCK>

<PCK INDEX=2
     PACKAGEID="OS/2 Netlabs\Netlabs EPM Distribution\Recompilation package\%ID_VERSION_NEPMD%"
     TARGET="$(1)"
     REQUIRES="1"
     SELECT
     TITLE="%PCK_2_TITLE%"

     CREATEOBJECT="WPProgram|%TITLE_NEPMD_RECOMP%|<NEPMD_FOLDER>|PROGTYPE=PM;EXENAME=$(1)\NETLABS\BIN\RECOMP.EXE;PARAMETERS=$(1)\MYEPM\EX;OBJECTID=<NEPMD_RECOMP>;"
     ><TINCLUDE name="..\nls\wis\desc2%NLS%.xml"/></PCK>

<PCK INDEX=3
     PACKAGEID="OS/2 Netlabs\Netlabs EPM Distribution\Programming samples\%ID_VERSION_NEPMD%"
     TARGET="$(1)"
     REQUIRES="1"
     SELECT
     TITLE="%PCK_3_TITLE%"

     CREATEOBJECT="WPFolder|%TITLE_NEPMD_NEPMD_SAMPLES_FOLDER%|<NEPMD_FOLDER>|CCVIEW=NO;OBJECTID=<NEPMD_SAMPLES_FOLDER>;"
     CREATEOBJECT="WPProgram|%TITLE_NEPMD_NEPMD_VIEW_EPMTK%|<NEPMD_SAMPLES_FOLDER>|PROGTYPE=PM;EXENAME=VIEW.EXE;PARAMETERS=$(1)\EPMBBS\SAMPLES\C\ETKE.INF;OBJECTID=<NEPMD_VIEW_EPMTK>;"
     CREATEOBJECT="WPProgram|%TITLE_NEPMD_NEPMD_VIEW_EPMCM_README%|<NEPMD_SAMPLES_FOLDER>|PROGTYPE=PM;EXENAME=E.EXE;PARAMETERS=$(1)\EPMBBS\SAMPLES\C\READ.ME;OBJECTID=<NEPMD_VIEW_EPMCM_README>;"
     CREATEOBJECT="WPProgram|%TITLE_NEPMD_NEPMD_VIEW_EPMRM_README%|<NEPMD_SAMPLES_FOLDER>|PROGTYPE=PM;EXENAME=E.EXE;PARAMETERS=$(1)\EPMBBS\SAMPLES\REXX\EPMREXX.DOC;OBJECTID=<NEPMD_VIEW_EPMRM_README>;"
     ><TINCLUDE name="..\nls\wis\desc3%NLS%.xml"/></PCK>

<PCK INDEX=4
     PACKAGEID="OS/2 Netlabs\Netlabs EPM Distribution\Speech Support\%ID_VERSION_NEPMD%"
     TARGET="$(1)"
     REQUIRES="1"
     TITLE="%PCK_4_TITLE%"

     CREATEOBJECT="WPProgram|%TITLE_NEPMD_NEPMD_SPEECH%|<NEPMD_FOLDER>|PROGTYPE=WINDOWABLEVIO;NINIMIZED=YES;EXENAME=$(1)\EPMBBS\BIN\EPMSPCH.EXE;STARTUPDIR=$(1)\EPMBBS\DLL;OBJECTID=<NEPMD_SPEECH>;"
     CREATEOBJECT="WPShadow|%TITLE_NEPMD_NEPMD_SPEECH_SHADOW%|<WP_START>|SHADOWID=<NEPMD_SPEECH>;OBJECTID=<NEPMD_SPEECH_SHADOW>;"
     CREATEOBJECT="WPShadow|%TITLE_NEPMD_NEPMD_WPSTART_SHADOW%|<NEPMD_FOLDER>|SHADOWID=<WP_START>;OBJECTID=<NEPMD_WPSTART_SHADOW>;"
     ><TINCLUDE name="..\nls\wis\desc4%NLS%.xml"/></PCK>

<PCK INDEX=99
     PACKAGEID="OS/2 Netlabs\Netlabs EPM Distribution\Base Application\%ID_VERSION_NEPMD%"
     TARGET="$(1)"
     REQUIRES="1"
     EXTERNAL="nepmd_epmbase.wpp"
     TITLE="%PCK_99_TITLE%"
     ><TINCLUDE name="..\nls\wis\desc99%NLS%.xml"/></PCK>

</HEAD>

<BODY>

<!--  ------------------------------ page 1: introductory page -------------------------------->
<PAGE INDEX=1 TYPE=TEXT>
<NEXTBUTTON TARGET=2>%BUTTON_NEXT%</NEXTBUTTON>
<TEXT><TINCLUDE name="..\nls\wis\page1%NLS%.xml"></TEXT>
</PAGE>

<!--  ------------------------------ page 2: credits pages ----------------------------------->
<PAGE INDEX=2 TYPE=TEXT>
<NEXTBUTTON TARGET=3>%BUTTON_NEXT%</NEXTBUTTON>
<TEXT><TINCLUDE name="..\nls\wis\page2%NLS%.xml"></TEXT>
</PAGE>

<!--  ------------------------------ page 3: Warpin restriction page ------------------------->
<PAGE INDEX=3 TYPE=TEXT>
<NEXTBUTTON TARGET=4>%BUTTON_NEXT%</NEXTBUTTON>
<TEXT><TINCLUDE name="..\nls\wis\page3%NLS%.xml"></TEXT>
</PAGE>

<!--  ------------------------------ page 4: show package container --------------------------->
<!-- The TYPE=CONTAINER will list the packages which can be installed. -->
<PAGE INDEX=4 TYPE=CONTAINER>
<NEXTBUTTON TARGET=5>%BUTTON_NEXT%</NEXTBUTTON>
<TEXT><TINCLUDE name="..\nls\wis\page4%NLS%.xml"></TEXT>
</PAGE>

<!--  ------------------------------ page 5: configure Warpin behaviour ----------------------->
<!-- The TYPE=CONFIGURE will change config.sys -->
<PAGE INDEX=5 TYPE=CONFIGURE>
<NEXTBUTTON TARGET=0>%BUTTON_NEXT%</NEXTBUTTON>
<TEXT><TINCLUDE name="..\nls\wis\page5%NLS%.xml"></TEXT>
</PAGE>

</BODY>
</WARPIN>

